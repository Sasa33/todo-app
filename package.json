{
  "name": "todo",
  "version": "1.0.0",
  "description": "Use electron and react to build a todo list application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "webpack-dev-server",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "package": "asar pack MyApp-darwin-x64/MyApp.app/Contents/Resources/app MyApp-darwin-x64/MyApp.app/Contents/Resources/electron.asar",
    "build": "electron-packager . MyApp --overwrite --icon=Icon.icns --prune=true",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=Icon.icns --prune=true --out=release-builds"
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-prebuilt": "^1.4.13",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "electron-packager": "^8.4.0",
    "electron-rebuild": "^1.4.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
